@model IEnumerable<BeyondTheTutor.Models.Tutor>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.BTTUser.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClassOf)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AdminApproved)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.BTTUser.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ClassOf)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AdminApproved)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
