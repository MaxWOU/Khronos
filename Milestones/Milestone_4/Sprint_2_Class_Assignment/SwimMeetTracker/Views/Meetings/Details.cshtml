@model SwimMeetTracker.Models.ViewModels.MeetingDetailsViewModel


@{
    ViewBag.Title = "Details";
}

<body class="other-bg-2">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-5" id="box-background">
                <h2 style="margin-top: 10px; color: #FFFFFF;">@Html.DisplayNameFor(model => model.eName): @Html.DisplayFor(model => model.eName)</h2>
                <h5 style="margin-top: 10px; color: #FFFFFF;">@Html.DisplayNameFor(model => model.eDate): @Html.DisplayFor(model => model.eDate)</h5>
            </div>
        </div>
    </div>
<br />
<table class="table table-striped table-sm">
    <thead>
        <tr>
            <th scope="col">@Html.DisplayNameFor(model => model.Teams)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.Names)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.EventTypes)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.CompletedIns)</th>
        </tr>
    </thead>
    <tbody>
        @{
            for (int i = 0; i < Model.Teams.Count() ; i++)
            {
                <tr>
                    <td>@Html.DisplayFor(model => model.Teams[i])</td>
                    <td>@Html.DisplayFor(model => model.Names[i])</td>
                    <td>@Html.DisplayFor(model => model.EventTypes[i])</td>
                    <td>@Html.DisplayFor(model => model.CompletedIns[i])<td>
                </tr>
            }
        }
    </tbody>
</table>
</body>
